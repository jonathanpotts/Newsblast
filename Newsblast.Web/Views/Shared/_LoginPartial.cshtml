@inject DiscordUserClient discordUserClient

@if (User.Identity.IsAuthenticated)
{
    var client = await discordUserClient.GetRestClientAsync();
    var user = client.CurrentUser;

    <ul class="navbar-nav">
        <li class="nav-item">
            <span class="nav-link">Hello, @user.Username#@user.Discriminator!</span>
        </li>
        <li class="nav-item">
            <a href="#" class="nav-link">Logout</a>
        </li>
    </ul>
}